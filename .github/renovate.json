{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":disableDependencyDashboard"],

  "assignees": ["jdno"],
  "reviewers": ["jdno"],

  "labels": ["C-dependency"],
  "semanticCommits": "disabled",

  "github-actions": {
    "fileMatch": [
      "ansible/*.yml",
      "github/*.yml",
      "json/*.yml",
      "markdown/*.yml",
      "rust/*.yml",
      "shell/*.yml",
      "terraform/*.yml",
      "typescript/*.yml",
      "yaml/*.yml"
    ],
    "labels": ["L-github"]
  }
}
